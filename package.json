{
  "name": "webpack-project",
  "version": "0.0.1",
  "description": "A sensible template for new JavaScript projects",
  "main": "./src/js/app.js",
  "scripts": {
    "setup": "node ./setup.js",
    "build:js": "node node_modules/webpack/bin/webpack.js",
    "build:sass": "node node_modules/gulp/bin/gulp.js",
    "watch:js": "node node_modules/webpack/bin/webpack.js --watch",
    "watch:sass": "node node_modules/gulp/bin/gulp.js watch",
    "clean": "rm -rf ./bin && rm -rf ./src",
    "cs": "npm run clean && npm run setup"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/championchap/webpack-template.git"
  },
  "keywords": [
    "template",
    "javascript",
    "webpack",
    "sass"
  ],
  "author": "Erik Watson",
  "license": "ISC",
  "homepage": "https://bitbucket.org/championchap/webpack-template#readme",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^1.6.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.15.1",
    "webpack": "^1.13.2"
  }
}
