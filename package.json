{
  "name": "frontend-template",
  "version": "0.0.1",
  "description": "A sensible, basic template for new Frontend projects",
  "main": "./src/js/app.js",
  "scripts": {
    "setup": "node ./setup.js",
    "build": "node node_modules/gulp/bin/gulp.js build",
    "build:js": "node node_modules/gulp/bin/gulp.js build:js",
    "build:sass": "node node_modules/gulp/bin/gulp.js build:sass",
    "watch": "node node_modules/gulp/bin/gulp.js watch",
    "watch:js": "node node_modules/gulp/bin/gulp.js watch:js",
    "watch:sass": "node node_modules/gulp/bin/gulp.js watch:sass",
    "clean": "rm -rf ./bin"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:championchap/Frontend-Template.git"
  },
  "keywords": [
    "template",
    "javascript",
    "webpack",
    "sass"
  ],
  "author": "Erik Watson",
  "license": "MIT",
  "homepage": "https://github.com/championchap/Frontend-Template",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-webpack": "^1.5.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.15.1",
    "webpack": "^1.13.2"
  }
}
